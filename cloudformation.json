{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Stelligent Demo",
  "Parameters": {
    "StelligentDemoBucket": {
      "Type": "String"
    },
    "StelligentDemoBucketURL": {
      "Type": "String"
    },
    "StelligentDemoBucketDomainName": {
      "Type": "String"
    },
    "StelligentDemoBucketCNAME": {
      "Type": "String"
    },
    "StelligentDemoVPC": {
      "Type": "String"
    },
    "StelligentDemoPublicSubnet": {
      "Type": "String"
    },
    "StelligentDemoPrivateSubnet1": {
      "Type": "String"
    },
    "StelligentDemoPrivateSubnet2": {
      "Type": "String"
    },
    "StelligentDemoPublicSecurityGroup": {
      "Type": "String"
    },
    "StelligentDemoPrivateSecurityGroup": {
      "Type": "String"
    },
    "StelligentDemoPrivateSecurityGroupSSH": {
      "Type": "String"
    },
    "StelligentDemoPrivateSecurityGroupMySQL": {
      "Type": "String"
    },
    "StelligentDemoDBHost": {
      "Type": "String"
    },
    "StelligentDemoDBName": {
      "Type": "String"
    },
    "StelligentDemoDBUser": {
      "Type": "String"
    },
    "StelligentDemoDBPass": {
      "Type": "String"
    },
    "InstanceTypeASG": {
      "Type": "String",
      "Default": "m1.small"
    },
    "InstanceTypeJenkins": {
      "Type": "String",
      "Default": "m1.medium"
    },
    "JenkinsAMI": {
      "Type": "String",
      "Default": "default"
    },
    "DemoRegion": {
      "Type": "String",
      "Default": "us-east-1"
    },
    "HashID": {
      "Type": "String"
    },
    "StelligentDemoJenkinsConfigSet": {
      "Type": "String",
      "Default": "default"
    },
    "StelligentDemoZoneName": {
      "Type": "String"
    },
    "KeyName": {
      "Type": "AWS::EC2::KeyPair::KeyName"
    },
    "StelligentDemoName": {
      "Type": "String"
    },
    "PrivateKey": {
      "Type": "String"
    },
    "CodeDeployAppName": {
      "Type": "String"
    },
    "CodeDeployDeploymentGroup": {
      "Type": "String"
    },
    "JenkinsUser": {
      "Type": "String"
    },
    "JenkinsEmail": {
      "Type": "String"
    },
    "JenkinsPassword": {
      "Type": "String"
    }
  },
  "Mappings": {
    "InstanceAMIs": {
      "us-east-1": {
        "AMI": "ami-1ccae774"
      },
      "us-west-2": {
        "AMI": "ami-ff527ecf"
      }
    },
    "AvailabilityZone": {
      "us-east-1": {
        "Public": "us-east-1c",
        "Private1": "us-east-1d",
        "Private2": "us-east-1e"
      },
      "us-west-2": {
        "Public": "us-west-2a",
        "Private1": "us-west-2b",
        "Private2": "us-west-2c"
      }
    },
    "S3Endpoints": {
      "us-east-1": {
        "Endpoint": "s3-external-1.amazonaws.com"
      },
      "us-west-1": {
        "Endpoint": "s3-us-west-1.amazonaws.com"
      },
      "us-west-2": {
        "Endpoint": "s3-us-west-2.amazonaws.com"
      },
      "eu-west-1": {
        "Endpoint": "s3-eu-west-1.amazonaws.com"
      },
      "eu-central-1": {
        "Endpoint": "s3-eu-central-1.amazonaws.com"
      },
      "ap-southeast-1": {
        "Endpoint": "s3-ap-southeast-1.amazonaws.com"
      },
      "ap-southeast-2": {
        "Endpoint": "s3-ap-southeast-2.amazonaws.com"
      },
      "ap-northeast-1": {
        "Endpoint": "s3-ap-northeast-1.amazonaws.com"
      },
      "sa-east-1": {
        "Endpoint": "s3-sa-east-1.amazonaws.com"
      }
    }
  },
  "Resources": {
    "StelligentDemoPublicLockedSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "GroupDescription": "stelligent-demo-public-locked",
        "VpcId": {
          "Ref": "StelligentDemoVPC"
        },
        "SecurityGroupIngress": []
      }
    },
    "StelligentDemoJenkinsStack": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "TemplateURL": {
          "Fn::Join": [
            "", ["https://", {
              "Fn::FindInMap": [
                "S3Endpoints", {
                  "Ref": "AWS::Region"
                },
                "Endpoint"
              ]
            }, "/", {
              "Ref": "StelligentDemoBucket"
            }, "/cloudformation.jenkins.json"]
          ]
        },
        "Parameters": {
          "InstanceTypeJenkins": {
            "Ref": "InstanceTypeJenkins"
          },
          "JenkinsAMI": {
            "Ref": "JenkinsAMI"
          },
          "DemoRegion": {
            "Ref": "DemoRegion"
          },
          "HashID": {
            "Ref": "HashID"
          },
          "CodeDeployAppName": {
            "Ref": "CodeDeployAppName"
          },
          "CodeDeployDeploymentGroup": {
            "Ref": "CodeDeployDeploymentGroup"
          },
          "StelligentDemoVPC": {
            "Ref": "StelligentDemoVPC"
          },
          "StelligentDemoZoneName": {
            "Ref": "StelligentDemoZoneName"
          },
          "StelligentDemoBucket": {
            "Ref": "StelligentDemoBucket"
          },
          "StelligentDemoStackName": {
            "Ref": "AWS::StackName"
          },
          "StelligentDemoPublicSubnet": {
            "Ref": "StelligentDemoPublicSubnet"
          },
          "StelligentDemoPublicSecurityGroup": {
            "Ref": "StelligentDemoPublicSecurityGroup"
          },
          "StelligentDemoPublicLockedSecurityGroup": {
            "Ref": "StelligentDemoPublicLockedSecurityGroup"
          },
          "StelligentDemoDBHost": {
            "Ref": "StelligentDemoDBHost"
          },
          "StelligentDemoDBName": {
            "Ref": "StelligentDemoDBName"
          },
          "StelligentDemoDBUser": {
            "Ref": "StelligentDemoDBUser"
          },
          "StelligentDemoDBPass": {
            "Ref": "StelligentDemoDBPass"
          },
          "JenkinsUser": {
            "Ref": "JenkinsUser"
          },
          "JenkinsEmail": {
            "Ref": "JenkinsEmail"
          },
          "JenkinsPassword": {
            "Ref": "JenkinsPassword"
          },
          "KeyName": {
            "Ref": "KeyName"
          },
          "PrivateKey": {
            "Ref": "PrivateKey"
          },
          "StelligentDemoJenkinsConfigSet": {
            "Ref": "StelligentDemoJenkinsConfigSet"
          }
        },
        "TimeoutInMinutes": "60"
      }
    },
    "StelligentDemoASGStack": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "TemplateURL": {
          "Fn::Join": [
            "", ["https://", {
              "Fn::FindInMap": [
                "S3Endpoints", {
                  "Ref": "AWS::Region"
                },
                "Endpoint"
              ]
            }, "/", {
              "Ref": "StelligentDemoBucket"
            }, "/cloudformation.asg.json"]
          ]
        },
        "Parameters": {
          "DemoRegion": {
            "Ref": "DemoRegion"
          },
          "HashID": {
            "Ref": "HashID"
          },
          "StelligentDemoZoneName": {
            "Ref": "StelligentDemoZoneName"
          },
          "StelligentDemoDBHost": {
            "Ref": "StelligentDemoDBHost"
          },
          "StelligentDemoDBName": {
            "Ref": "StelligentDemoDBName"
          },
          "StelligentDemoDBUser": {
            "Ref": "StelligentDemoDBUser"
          },
          "StelligentDemoDBPass": {
            "Ref": "StelligentDemoDBPass"
          },
          "StelligentDemoPublicSubnet": {
            "Ref": "StelligentDemoPublicSubnet"
          },
          "StelligentDemoPublicSecurityGroup": {
            "Ref": "StelligentDemoPublicSecurityGroup"
          },
          "InstanceTypeASG": {
            "Ref": "InstanceTypeASG"
          },
          "KeyName": {
            "Ref": "KeyName"
          }
        },
        "TimeoutInMinutes": "60"
      }
    }
  },
  "Outputs": {
    "StelligentDemoName": {
      "Value": {
        "Ref": "StelligentDemoName"
      }
    },
    "StelligentDemoBucket": {
      "Value": {
        "Ref": "StelligentDemoBucket"
      }
    },
    "StelligentDemoWebASG": {
      "Value": {
        "Fn::GetAtt": ["StelligentDemoASGStack", "Outputs.StelligentDemoWebASG"]
      }
    },
    "StelligentDemoJenkinsEIP": {
      "Value": {
        "Fn::GetAtt": ["StelligentDemoJenkinsStack", "Outputs.StelligentDemoJenkinsEIP"]
      }
    },
    "StelligentDemoDBName": {
      "Value": {
        "Ref": "StelligentDemoDBName"
      }
    },
    "StelligentDemoDBUser": {
      "Value": {
        "Ref": "StelligentDemoDBUser"
      }
    },
    "StelligentDemoDBPass": {
      "Value": {
        "Ref": "StelligentDemoDBPass"
      }
    },
    "StelligentDemoDBHost": {
      "Value": {
        "Ref": "StelligentDemoDBHost"
      }
    },
    "StelligentDemoJenkinsURL": {
      "Value": {
        "Fn::GetAtt": ["StelligentDemoJenkinsStack", "Outputs.StelligentDemoJenkinsURL"]
      }
    },
    "StelligentDemoELBDNSNAME": {
      "Value": {
        "Fn::GetAtt": ["StelligentDemoASGStack", "Outputs.StelligentDemoELBDNSNAME"]
      }
    },
    "StelligentDemoBucketURL": {
      "Value": {
        "Ref": "StelligentDemoBucketURL"
      }
    },
    "StelligentDemoCNAME": {
      "Value": {
        "Fn::Join": [
          ".", [{
            "Fn::Join": [
              "-", [
                "http://stelligent-demo", {
                  "Ref": "DemoRegion"
                }, {
                  "Ref": "HashID"
                }
              ]
            ]
          }, {
            "Ref": "StelligentDemoZoneName"
          }]
        ]
      }
    },
    "StelligentDemoBucketCNAME": {
      "Value": {
        "Ref": "StelligentDemoBucketCNAME"
      }
    },
    "StelligentDemoJenkinsCNAME": {
      "Value": {
        "Fn::Join": [
          "", [
            "http://stelligent-demo-jenkins-", {
              "Ref": "DemoRegion"
            },
            "-", {
              "Ref": "HashID"
            },
            ".", {
              "Ref": "StelligentDemoZoneName"
            },
            ":8080"
          ]
        ]
      }
    },
    "SSHCommandToJenkins": {
      "Value": {
        "Fn::GetAtt": ["StelligentDemoJenkinsStack", "Outputs.SSHCommandToJenkins"]
      }
    }
  }
}
