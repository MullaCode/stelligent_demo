<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>nando-automation-demo</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@2.3.5">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/stelligent/nando_automation_demo</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.SCMTrigger>
      <spec>* * * * *</spec>
      <ignorePostCommitHooks>false</ignorePostCommitHooks>
    </hudson.triggers.SCMTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.plugins.python.Python plugin="python@1.2">
      <command>import os
from instagram.client import InstagramAPI

api = InstagramAPI(client_id=&apos;4d456b2f3f27436ab95f8a476171d739&apos;)
popular_media = api.media_popular(count=1)
os.system(&apos;wget -O image1.jpg %s&apos; % (popular_media[0].images[&apos;thumbnail&apos;].url) )
popular_media = api.media_popular(count=1)
os.system(&apos;wget -O image2.jpg %s&apos; % (popular_media[0].images[&apos;thumbnail&apos;].url) )
</command>

    </hudson.plugins.python.Python>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>
